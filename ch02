
#include <stdio.h>

void jolly(void);
void deny(void);
void br(void);
void ic(void);
void smile(void);
void one_three(void);
void two(void);

int main(void)
{
    // 01
    printf("Begae P\n");
    printf("Begae \nP\n");
    printf("Begae ");
    printf("P\n");
    
    //02
    printf("Begae P\n");
    printf("Daegu, Korea\n");
    
    // 03
    int year_age, date_age;
    year_age = 25;
    date_age = year_age * 365;
    printf("Year Age: %i, Date Age: %i\n", year_age, date_age);
    
    // 04
    jolly();
    jolly();
    jolly();
    deny();
    
    // 05
    br(); printf(", "); ic(); printf("\n");
    ic(); printf(", \n");
    br(); printf("\n");
    
    // 06
    int toes, doubled, squared;
    toes = 10;
    doubled = 2 * toes;
    squared = toes * toes;
    printf("Toes: %i, Doubled: %i, Squared: %i\n", toes, doubled, squared);
    
    // 07
    smile(); smile(); smile(); printf("\n");
    smile(); smile(); printf("\n");
    smile(); printf("\n");
    
    // 08
    printf("starting now:\n");
    one_three();
    printf("done!\n");
    
    return 0;
}

void jolly(void)
{
    printf("For he's a jolly good fellow!\n");
}

void deny(void)
{
    printf("Which nobody can deny!\n");
}

void br(void)
{
    printf("Brazil, Russia");
}

void ic(void)
{
    printf("India, China");
}

void smile(void)
{
    printf("Smile!");
}

void one_three(void)
{
    printf("one\n");
    two();
    printf("three\n");
}

void two(void)
{
    printf("two\n");
}

